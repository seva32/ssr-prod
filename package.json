{
  "name": "react-server-webpack",
  "version": "0.0.0",
  "description": "React apps",
  "main": "src/index.js",
  "engines": {
    "node": "14.2.0"
  },
  "scripts": {
    "clean": "rm -rf build",
    "lint": "eslint src --ext .js*",
    "build:server": "node -r @babel/register node_modules/.bin/webpack -p --config webpack/webpack.server.config.js",
    "start:dev": "NODE_ENV=production nodemon --trace-deprecation server/index.js",
    "dev": "yarn run clean && yarn run build:server && yarn run start:dev",
    "pro": "rm -rf build && node -r @babel/register node_modules/.bin/webpack -p --config webpack/webpack.prod.config.js && NODE_ENV=production node --trace-deprecation server/index.js",
    "test": "export NODE_ENV=test && jest --watch",
    "prod1": "node -r @babel/register node_modules/.bin/webpack -p --config webpack/webpack.prod.config.js --mode production",
    "prod": "rm -rf build && rm -rf public && yarn run prod1 && yarn run build:server --env.MONGOOSE=$(grep MONGOOSE .env | cut -d '=' -f2) --env.PORT=$(grep PORT .env | cut -d '=' -f2) --env.TOKEN_SECRET=$(grep TOKEN_SECRET .env | cut -d '=' -f2) --env.REACT_APP_GOOGLE_ID=$(grep REACT_APP_GOOGLE_ID .env | cut -d '=' -f2) && NODE_ENV=production node build/bundle.js"
  },
  "author": "seva32",
  "private": true,
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js"
    ],
    "coverageReporters": [
      "text"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|less)$": "<rootDir>/src/utils/jest/CSSStub.js",
      "^[./a-zA-Z0-9$_-]+\\.jpg$": "<rootDir>/src/utils/jest/ImgStub.js"
    },
    "displayName": {
      "name": "PROJECT",
      "color": "blue"
    }
  },
